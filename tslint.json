{
  "enable": true,
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "import-name": false,
    "no-default-export": true,
    "jsx-no-lambda": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
